color03:	
	li $t0, 40
	addi $a0, $a0, -1

	#Stores the Color Needed to Be Used
	lw	$a2, colors($t0)

	#Colors the Pixel
	jal	draw_pixel

	#Changes the Value of image1array at the Position to the New Color
	li $t8, 256
	mult $a1, $t8
	mflo $a3
	subi $a3, $a3, 4
	li $t8, 4
	mult $a0, $t8
	mflo $t7
	add $a3, $a3, $t7
	li $t8, 10
	sw $t8, image3array($a3)
	addi 	$a2, $0, GRAY1

	#Draws the Gray Pixel 1 Right of the Colored Pixel
	jal	draw_pixel
	j	loop3

#Colors the Pixel White

color93:	
	li $t0, 76
	addi $a0, $a0, -1

	#Stores the Color Needed to Be Used
	lw	$a2, colors($t0)

	#Colors the Pixel
	jal	draw_pixel

	#Changes the Value of image1array at the Position to the New Color
	li $t8, 256
	mult $a1, $t8
	mflo $a3
	subi $a3, $a3, 4
	li $t8, 4
	mult $a0, $t8
	mflo $t7
	add $a3, $a3, $t7
	li $t8, 19
	sw $t8, image3array($a3)
	addi 	$a2, $0, GRAY1

	#Draws the Gray Pixel 1 Right of the Colored Pixel
	jal	draw_pixel
	j	loop3

draw_pixel:
	# s1 = address = $gp + 4*(x + y*width)
	mul	$t9, $a1, WIDTH   # y * WIDTH
	add	$t9, $t9, $a0	  # add X
	mul	$t9, $t9, 4	  # multiply by 4 to get word offset
	add	$t9, $t9, $gp	  # add to base address
	sw	$a2, ($t9)	  # store color at memory location
	addi $a0, $a0, 1
	jr 	$ra

#Exit Function
exit:
  li	$v0, 10
	syscall
